    function adjustWorkingTimeFields(state) {
		if (state == 1) {
			$("select#day_id_"+area_id).attr('disabled',false);
			$("#uniform-day_id_"+area_id).removeClass('disabled');
			$("input#time_from_"+area_id).attr('disabled',false);
			$("input#time_to_"+area_id).attr('disabled',false);
			$("#slider-range").slider( "option", "disabled", false );
		}
		else {
			$("select#day_id_"+area_id).attr('disabled',true);
			$("#uniform-day_id_"+area_id).addClass('disabled');
			$("input#time_from_"+area_id).attr('disabled',true);
			$("input#time_to_"+area_id).attr('disabled',true);
			$("#slider-range").slider( "option", "disabled", true );
		}    	
    }
	
