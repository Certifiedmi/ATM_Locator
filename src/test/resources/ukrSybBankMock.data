    $("#region_id_"+area_id).change(function(){
        $("#city_id_"+area_id).empty().attr("disabled", true);
        $("#station_id_"+area_id).empty().attr("disabled", true);
        json = {"region_id": $(this).val(), "list": "cities", "ajax": "true"};
        json["t_" + area_id] = new Date().getTime();                
        $.getJSON("index.php", json, function(data){
            var options = $("#city_id_"+area_id).attr('options');
            options[options.length] = new Option(sysw_all_cities, 0, false, false);
            $.each(data, function(){
                var selected = city_id == this.BRANCH_CITY_ID;                            
                options[options.length] = new Option(this.TITLE, this.BRANCH_CITY_ID, selected, selected);
            });
            $("#city_id_"+area_id).attr("disabled", false);
            $("#city_id_"+area_id).trigger("change");
        });
        $("#address_"+area_id).val('');
        $("#address_"+area_id).autocomplete( "option" , 'source' , makeAddressAutocompleteURL() );
        /* проверка состояния глобальной переменной, если трушное значение значит нужно обнулять поля формы */
        if(gl_change){
        	$("#address_"+area_id).val('');
	        document.getElementById('hidden_lat').value = 0;
	        document.getElementById('hidden_lng').value = 0;
	        document.getElementById('hidden_zoom').value = 0;
	        document.getElementById('branch_title_id_' + area_id).value = '';
        }
        /*****************************************/
        
    });
    
    $("#city_id_"+area_id).change(function(){
        //$("#is_subway_"+area_id).attr("checked", false).attr("disabled", true);
        $("#station_id_"+area_id).empty().attr("disabled", true);                            
        $("#station_id_"+area_id).parent('.selector').addClass('disabled');       
                 
        json = {"city_id": $(this).val(), "branch_id" : "", "list": "stations", "ajax": "true"};
        json["t_" + area_id] = new Date().getTime();
        $.getJSON("index.php", json, function(data){
            $('#station_id_'+area_id).empty();
            var options = $('#station_id_'+area_id).attr('options');
            options[options.length] = new Option(sysw_all_stations, 0, false, false)
            $.each(data, function(){
                var selected = this.CITY_SUBWAY_STATION_ID == station_id;
                //$("#is_subway_"+area_id).attr("checked", selected ? true : $("#is_subway_"+area_id).attr("checked"));
                options[options.length] = new Option(this.TITLE, this.CITY_SUBWAY_STATION_ID, selected, selected);                
            });
            if (options.length > 1)
            {
                $("#is_subway_"+area_id).attr("disabled", false);
                $("#station_id_"+area_id).attr("disabled", false);
                $("#station_id_"+area_id).parent('.selector').removeClass('disabled'); 
            }
            $("#station_id_"+area_id).trigger("change");   
            
        });
        $("#address_"+area_id).val('');
        //убираем неиспользуемую функцию Автокомплита
        //$("#address_"+area_id).autocomplete( "option" , 'source' , makeAddressAutocompleteURL() );
    });
        
    $("#is_subway_"+area_id).change(function(){
        if ($("#is_subway_"+area_id).attr('checked'))
        {
            $("#is_subway_hidden_"+area_id).val(1);
//            $("#station_id_"+area_id).attr("disabled", false);                    
        }
        else
        {
            $("#is_subway_hidden_"+area_id).val(0);
//            $("#station_id_"+area_id).attr("disabled", true);                    
        }
        $("#station_id_"+area_id).trigger("change");        
    });
    
    $('#is_subway_"+area_id').click(function(){
      $('.target').change();
    });
    
    $("#station_id_"+area_id).change(function(){
        $("#station_id_hidden_"+area_id).val($("#station_id_"+area_id).attr("disabled") ? 
            0 : $("#station_id_"+area_id+" option:selected").val());
    });

    $("#type_branch_"+area_id).change(function() {
        if (this.checked) {
            $(".b_box").each(function(){this.disabled = 0; if ($(this).val() != 10) $(this).attr('checked', true);})
        } else {
            $(".b_box").each(function(){this.disabled = 1;})
        }
    });
    $("#type_atm_"+area_id).change(function() {
        if (this.checked) {
            $(".a_box").each(function(){this.disabled = 0; $(this).attr('checked', true);})
        } else {
            $(".a_box").each(function(){this.disabled = 1;})
        }
    });
    $(document).ready(function(){
    	
        if ($("#type_branch_"+area_id).get(0).checked) {
            $(".b_box").each(function(){this.disabled = 0;})
        } else {
            $(".b_box").each(function(){this.disabled = 1;})
        }
        if ($("#type_atm_"+area_id).get(0).checked) {
            $(".a_box").each(function(){this.disabled = 0;})
        } else {
            $(".a_box").each(function(){this.disabled = 1;})
        }
    });
    

    
</script>
        		
