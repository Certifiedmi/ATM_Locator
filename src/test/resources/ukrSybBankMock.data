              (place.address_components[0] && place.address_components[0].short_name || ''),
              (place.address_components[1] && place.address_components[1].short_name || ''),
              (place.address_components[2] && place.address_components[2].short_name || '')
            ].join(' ');
          }
          infowindow.setContent('<div><strong>' + place.name + '</strong><br>' + address);
          infowindow.open(map, marker);
          gl_change = false;
          document.getElementById('hidden_lat').value = place.geometry.location.lat()
          document.getElementById('hidden_lng').value = place.geometry.location.lng();
          document.getElementById('hidden_zoom').value = map.zoom;
          document.getElementById('branch_title_id_' + area_id).value = '';
          $("#region_id_"+area_id).val("-1");
          $("#region_id_"+area_id).trigger('change');
          
          //$("#address_id_"+area_id).val(address);
          //$("#address_id_"+area_id).trigger('change');
          
          $("#city_id_"+area_id).empty().attr("disabled", true);
       	  $("#station_id_"+area_id).empty().attr("disabled", true);
       	  
          
       	  gl_change = true;
       	  setTimeout(function(){
		 // 	alert(address);
		  document.getElementById("address_"+area_id).value = new_value_address;	
		 // document.getElementById("address_"+area_id).value = address;	
		  //alert(address);
		  document.getElementById('hidden_lat').form.onsubmit = function(){return true;};
       	  	document.getElementById('hidden_lat').form.submit();      
		   }, 200);
		    });
		 google.maps.event.addListener(map, 'zoom_changed', function() {
			//неиспользуемый обработчик зуммирования карты для отладки
		  });
