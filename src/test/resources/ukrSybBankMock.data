          document.getElementById('hidden_lng').value = 0;
          document.getElementById('hidden_zoom').value = 0;
          $("#region_id_"+area_id).val("-1");
          $("#region_id_"+area_id).trigger('change');
          $("#city_id_"+area_id).empty().attr("disabled", true);
